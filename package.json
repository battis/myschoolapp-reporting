{
  "name": "myschoolapp-reporting",
  "version": "0.3.1",
  "keywords": [
    "Blackbaud",
    "Podium",
    "MySchoolApp",
    "LMS",
    "export",
    "reporting",
    "analytics"
  ],
  "homepage": "https://github.com/battis/myschoolapp-reporting#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/battis/myschoolapp-reporting.git"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "Seth Battis",
    "url": "https://github.com/battis"
  },
  "type": "module",
  "bin": {
    "msar": "./dist/bin/msar.js"
  },
  "scripts": {
    "debug": "run-s debug:*",
    "debug:build": "run-s build",
    "debug:inspect": "source .env && node inspect ./dist/bin/msar.js snapshot -u \"$(op item get $OP_ITEM --fields username)\" -p \"$(op item get $OP_ITEM --fields password --reveal)\" --sso entra-id --outputPath ./var \"$URL\"",
    "clean": "del ./dist",
    "build": "run-s build:*",
    "build:clean": "run-s clean",
    "build:compile": "tsc",
    "release": "run-s release:*",
    "release:build": "run-s build",
    "release:changeset": "changeset version",
    "release:publish": "pnpm publish -r --otp $(op item get npm --otp)"
  },
  "prettier": "@battis/prettier-config",
  "dependencies": {
    "@battis/qui-cli": "^0.8.4",
    "@battis/typescript-tricks": "^0.6.1",
    "@oauth2-cli/sky-api": "^0.1.0",
    "async-mutex": "^0.5.0",
    "content-disposition": "^0.5.4",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "expressionparser": "^1.1.5",
    "json-2-csv": "^5.5.7",
    "mime": "^4.0.4",
    "puppeteer": "^23.10.1"
  },
  "devDependencies": {
    "@battis/eslint-config": "latest",
    "@battis/prettier-config": "latest",
    "@changesets/cli": "^2.27.10",
    "@tsconfig/node20": "^20.1.4",
    "@types/content-disposition": "^0.5.8",
    "@types/express": "^5.0.0",
    "del-cli": "^6.0.0",
    "globals": "^15.13.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
}
